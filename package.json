    "serve-favicon": "^2.5.0",
    "source-map-support": "^0.5.19",
    "spotify-web-api-node": "^4.0.0",
    "ts-node": "^8.10.2",
    "twitch": "^4.0.10",
    "twitch-chat-client": "^4.1.0-pre.10",
    "vue": "^2.6.11",
    "vue-clipboard2": "^0.3.1",
