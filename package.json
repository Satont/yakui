{
  "name": "yakui",
  "version": "4.0.0",
  "description": "Yet another Twitch bot written on typescript",
  "author": "Satont <satontworldwide@gmail.com>",
  "scripts": {
    "prestart": "npm run migration:up",
    "start": "cross-env NODE_ENV=production node --experimental-report --report-on-fatalerror --report-directory=./logs/ dist/index.js",
    "debug": "node --inspect=0.0.0.0:9229 --nolazy ./dist/index.js",
    "build": "npm run build:web && npm run build:bot",
    "dev": "npm run dev:web & npm run dev:bot",
    "dev:bot": "nodemon",
    "dev:web": "webpack --config webpack.ts --watch --progress",
    "build:web": "cross-env NODE_ENV=production webpack --config webpack.ts",
    "build:bot": "cross-env NODE_ENV=production tsc --project src/bot/tsconfig.json",
    "inspector": "cross-env NODE_ENV=production node --experimental-report --report-on-fatalerror --report-directory=./logs/ --inspect=0.0.0.0:9229 --nolazy dest/index",
    "eslint": "npx eslint --ext .ts src",
    "eslint:fix": "npx eslint --ext .ts --fix src",
    "migration:create": "prisma migrate dev",
    "migration:up": "prisma migrate deploy",
    "prebuild:bot": "rimraf dist",
    "prisma": "prisma",
    "prisma:resolveinitial": "prisma migrate resolve --applied 20210703215017_initial_migration"
  },
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "@types/connect-history-api-fallback": "^1.3.5",
    "@types/css-minimizer-webpack-plugin": "^3.2.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/humanize-duration": "^3.27.0",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/lodash": "^4.14.177",
    "@types/mini-css-extract-plugin": "^2.4.0",
    "@types/moment-precise-range-plugin": "^0.2.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.10",
    "@types/pg": "^8.6.1",
    "@types/serve-favicon": "^2.5.3",
    "@types/socket.io": "^3.0.2",
    "@types/spotify-web-api-node": "^5.0.4",
    "@types/terser-webpack-plugin": "^5.2.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.9.0",
    "@vue/component-compiler-utils": "^3.3.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "eslint": "^8.3.0",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.15",
    "prisma": "^3.5.0",
    "terser-webpack-plugin": "^5.2.5",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "vue-loader": "^15.9.8",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@prisma/client": "^3.5.0",
    "@satont/text-utils": "^1.0.8",
    "@twurple/api": "^5.0.9",
    "@twurple/auth": "^5.0.9",
    "@twurple/chat": "^5.0.9",
    "@twurple/eventsub": "^5.0.9",
    "@twurple/pubsub": "^5.0.9",
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
