{
  "name": "yakui",
  "version": "4.0.0",
  "description": "Yet another Twitch bot written on typescript",
  "author": "Satont <satontworldwide@gmail.com>",
  "scripts": {
    "prestart": "npm run migration:up",
    "start": "cross-env NODE_ENV=production node --experimental-report --report-on-fatalerror --report-directory=./logs/ --always_compact --optimize_for_size --gc_interval=100 dest/index.js",
    "build": "npm run build:web && npm run build:bot",
    "dev": "npm run dev:web & npm run dev:bot",
    "dev:bot": "nodemon",
    "dev:web": "webpack --config webpack.ts --watch --progress",
    "build:web": "cross-env NODE_ENV=production webpack --config webpack.ts",
    "build:bot": "cross-env NODE_ENV=production tsc --project src/bot/tsconfig.json",
    "inspector": "cross-env NODE_ENV=production node --experimental-report --report-on-fatalerror --report-directory=./logs/ --always_compact --optimize_for_size --gc_interval=100 --inspect=0.0.0.0:9229 --nolazy dest/index",
    "eslint": "npx eslint --ext .ts src",
    "eslint:fix": "npx eslint --ext .ts --fix src",
    "migration:create": "mikro-orm migration:create",
    "migration:up": "mikro-orm migration:up",
    "migration:down": "mikro-orm migration:down"
  },
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/connect-history-api-fallback": "^1.3.3",
    "@types/css-minimizer-webpack-plugin": "^1.1.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/humanize-duration": "^3.18.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.161",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/moment-precise-range-plugin": "^0.2.0",
    "@types/node": "^14.14.6",
    "@types/pg": "^7.14.5",
    "@types/serve-favicon": "^2.5.0",
    "@types/socket.io": "^2.1.11",
    "@types/spotify-web-api-node": "^4.0.1",
    "@types/terser-webpack-plugin": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "@vue/component-compiler-utils": "^3.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-minimizer-webpack-plugin": "^1.1.4",
    "eslint": "^7.9.0",
    "html-webpack-plugin": "^4.4.1",
    "nodemon": "^2.0.4",
    "terser-webpack-plugin": "^4.2.2",
    "ts-loader": "^8.0.9",
    "typescript": "^4.0.5",
    "vue-loader": "^15.9.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@mikro-orm/cli": "^4.0.7",
    "@mikro-orm/core": "^4.0.7",
    "@mikro-orm/postgresql": "^4.0.7",
    "@mikro-orm/reflection": "^4.0.7",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.2",
    "bootstrap-vue": "^2.18.1",
    "centrifuge": "^2.6.4",
    "chalk": "^4.1.0",
    "connect-history-api-fallback": "^1.6.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "humanize-duration": "^3.23.1",
    "i": "^0.3.6",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.5",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^0.11.2",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "moment-precise-range-plugin": "^1.3.0",
    "npm": "^6.14.8",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "rotating-file-stream": "^2.1.3",
    "safe-eval": "^0.4.1",
    "serve-favicon": "^2.5.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "source-map-support": "^0.5.19",
    "spotify-web-api-node": "^4.0.0",
    "strip-ansi": "^6.0.0",
    "tlds": "^1.210.0",
    "ts-morph": "^8.1.1",
    "ts-node": "^9.0.0",
    "twitch": "^4.2.7",
    "twitch-auth": "^4.2.2",
    "twitch-chat-client": "^4.2.7",
    "twitch-pubsub-client": "^4.2.2",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-clipboard2": "^0.3.1",
    "vue-grid-layout": "^2.3.8",
    "vue-plugin-load-script": "^1.3.2",
    "vue-property-decorator": "^9.0.0",
    "vue-router": "^3.4.3",
    "vue-socket.io-extended": "^4.0.4",
    "vue-toastification": "^1.7.7",
    "vuex": "^3.5.1",
    "ws": "^7.3.1"
  },
  "_moduleAliases": {
    "@bot": "dest/",
    "@": "./",
    "@src": "./src"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/bot/mikro-orm.config.ts",
      "./dest/mikro-orm.config.js"
    ]
  }
}
