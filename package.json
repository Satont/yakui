{
  "name": "yakui",
  "version": "3.0.0",
  "description": "",
  "scripts": {
    "migrate": "knex migrate:latest",
    "prestart": "knex migrate:latest",
    "start": "cross-env NODE_ENV=production node --experimental-report --report-on-fatalerror --report-directory=./logs/ --always_compact --optimize_for_size --gc_interval=100 dest/index",
    "build": "npm run build:web && npm run build:bot",
    "dev": "npm run dev:web & npm run dev:bot",
    "dev:bot": "nodemon",
    "dev:web": "webpack --config webpack.ts --watch --progress",
    "build:web": "webpack --config webpack.ts",
    "build:bot": "tsc --project src/bot/tsconfig.json",
    "inspector": "cross-env NODE_ENV=production node --experimental-report --report-on-fatalerror --report-directory=./logs/ --always_compact --optimize_for_size --gc_interval=100 --inspect=0.0.0.0:9229 --nolazy dest/index"
  },
  "engines": {
    "node": ">=12"
  },
