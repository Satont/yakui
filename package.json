{
  "name": "twobit",
  "version": "3.0.0",
  "description": "",
  "scripts": {
    "migrate": "knex migrate:latest",
    "prestart": "knex migrate:latest",
    "start": "cross-env NODE_ENV=production node --experimental-report --report-on-fatalerror --report-directory=./logs/ --always_compact --optimize_for_size --gc_interval=100 dest/index",
    "build": "npm run build:web && npm run build:bot",
    "dev": "npm run dev:web & npm run dev:bot",
    "dev:bot": "nodemon",
    "dev:web": "webpack --config webpack.ts --watch --progress",
    "build:web": "webpack --config webpack.ts",
    "build:bot": "tsc --project src/bot/tsconfig.json",
    "inspector": "cross-env NODE_ENV=production node --experimental-report --report-on-fatalerror --report-directory=./logs/ --always_compact --optimize_for_size --gc_interval=100 --inspect=0.0.0.0:9229 --nolazy dest/index"
  },
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/connect-history-api-fallback": "^1.3.3",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/humanize-duration": "^3.18.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.155",
    "@types/node": "^14.0.12",
    "@types/serve-favicon": "^2.5.0",
    "@types/spotify-web-api-node": "^4.0.1",
    "@types/tmi.js": "^1.4.0",
    "@vue/component-compiler-utils": "^3.1.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "html-webpack-plugin": "^4.3.0",
    "nodemon": "^2.0.4",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "vue-loader": "^15.9.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.0",
    "bootstrap-vue": "^2.15.0",
    "centrifuge": "^2.6.2",
    "chalk": "^4.0.0",
    "connect-history-api-fallback": "^1.6.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.5.0",
    "humanize-duration": "^3.23.0",
